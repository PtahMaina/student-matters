extends base

mixin navItem(link, text, icon, active)
  li(class=`${active ? 'side-nav--active' : ''}`)
    a(href=`${link}`)
      svg
        use(xlink:href=`img/icons.svg#icon-${icon}`)
      | #{text}

block content
  .form.special-form
    ul1
      li
        a(href='/student-login') Student Login
      li
        a(href='/staff-login') Staff Login
    